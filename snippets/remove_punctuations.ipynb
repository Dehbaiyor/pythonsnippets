    "import re\n",
    "\n",
    "def remove_punctuations(value):\n",
    "    return re.sub('[#!?]','',value)\n",
    "     \n",
    "\n",
    "states = [' Alabama ', 'Georgia!', 'Georgia', 'geo','south carolina##', 'West virginia?']\n",
    "#remove_punctuations(states)\n",
    "list(map(remove_punctuations,states))\n",
    "    "
